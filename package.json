{
  "name": "opencode-acp-skill",
  "type": "module",
  "private": true,
  "scripts": {
    "conf": "CLAWDBOT_STATE_DIR=./clawdbot/state CLAWDBOT_CONFIG_PATH=./clawdbot/config.json bun clawdbot --dev config",
    "gate": "CLAWDBOT_STATE_DIR=./clawdbot/state CLAWDBOT_CONFIG_PATH=./clawdbot/config.json bun clawdbot --dev gateway",
    "tui": "CLAWDBOT_STATE_DIR=./clawdbot/state CLAWDBOT_CONFIG_PATH=./clawdbot/config.json bun clawdbot --dev tui",
    "doctor": "CLAWDBOT_STATE_DIR=./clawdbot/state CLAWDBOT_CONFIG_PATH=./clawdbot/config.json bun clawdbot --dev doctor",
    "dry": "clawdhub sync --root src/skills --all --dry-run",
    "release": "clawdhub sync --root src/skills --all"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "clawdbot": "^2026.1.11-4",
    "clawdhub": "^0.2.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "trustedDependencies": [
    "@whiskeysockets/baileys",
    "clawdbot",
    "protobufjs"
  ]
}
